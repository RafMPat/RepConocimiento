    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <rdf:Description rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/f">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/old">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/w">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/any">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/b">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>ComprobarEsViejo</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Wine"/>
                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/w"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#anyo"/>
                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/w"/>
                                <swrl:argument2 rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia/any"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
