        <madeWith rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Tinta_del_Pais"/>
        <anyo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2021</anyo>
        <body rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</body>
        <isOld rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isOld>
        <nombre>Peñafiel</nombre>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Polverete -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Polverete">
        <rdf:type rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Chardonnay"/>
        <anyo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</anyo>
        <body rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</body>
        <nombre>Polverete</nombre>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Ribera_del_duero -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Ribera_del_duero">
        <rdf:type rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Spanish_Vineyard"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Tinta_del_Pais -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Tinta_del_Pais">
        <rdf:type rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Red_Grape"/>
        <growingAs rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Ribera_del_duero"/>
    </owl:NamedIndividual>
    


    <!-- 
