    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Clarete -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Clarete">
        <rdf:type rdf:resource="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Rose"/>
        <anyo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2021</anyo>
        <body rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</body>
        <descripcion>Wine made in Spain</descripcion>
        <nombre>Clarete</nombre>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Penyafiel -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/raf/ontologies/2023/3/enologia#Penyafiel">
